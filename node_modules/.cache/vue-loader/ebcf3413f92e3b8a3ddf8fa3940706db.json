ree = setTimeout(()=>{\n                                this.thanksText = '3'\n                            this.$refs.thanksDiv.style.opacity = 1;\n                                let two = setTimeout(()=>{\n                                        clearTimeout(three)\n                                        this.thanksText = '2'\n\n                                        let one = setTimeout(()=>{\n                                                clearTimeout(two)\n                                                this.thanksText = '1'\n\n                                                setTimeout(()=>{\n                                                        clearTimeout(one)\n                                                        console.log(\"push\")\n                                                        router.push({ name : 'MainPage'})\n                                                    },1000\n                                                )\n                                            }, 1000\n                                        )\n                                    }, 1000\n                                )\n                            }, 1000\n                        )\n                        clearTimeout(setTime_func);\n                    }\n\n                }\n            },\n            bankPointerFadeIn() {\n\n                this.opacity -= 0.05\n                this.$refs.thanksDiv.style.opacity = this.opacity;\n                this.bankPointerHide()\n            }\n        },\n        data(){\n            return {\n                opacity : 0,\n                fadeCycle : 0,\n                thanksText : '감사합니다!'\n            }\n        }\n    }\n</script>\n<style>\n    .content{\n        width: 70%;\n    }\n    .thanksDiv{\n        width: 100%;\n        text-align:center;\n        padding-top: 20%;\n        font-size: 50px;\n        opacity: 0;\n    }\n</style>\n\n\n\n\n"]}]}